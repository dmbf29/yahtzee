<div class="rolls-container">
  <table class="table table-hover table-sm" style="margin-bottom: 0;">
    <thead class="thead-dark">
      <tr>
        <th scope="col">Last Rolls</th>
      </tr>
    </thead>
  </table>
  <div>
    <div id="rolls" data-gameroll-id="<%= @game.id %>">
      <% @game.submissions.order(updated_at: :desc).each do |submission| %>
        <%= render "submissions/message", submission: submission %>
      <% end %>
    </div>
  </div>
</div>
